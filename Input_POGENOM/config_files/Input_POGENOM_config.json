  "samtools_view_alignment_extra_filters": "-f 2 -F 1024",
  "freebayes_parameters": "-C 4 -p 1 --pooled-continuous --read-max-mismatch-fraction 0.05 --min-alternate-fraction 0.01 -q 15 --report-monomorphic",
