{
  "workdir": "/root/POGENOM/Input_POGENOM",
  "dataset": "TEST",
  "min_coverage": 20,
  "min_breadth": 40,
  "subsample": "FALSE",
  "min_bsq_for_cov_median_calculation": 15,
  "mode_prefilt": "FALSE",
